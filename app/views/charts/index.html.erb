<div id="usage" style="width:100%; height:400px;"></div>
<br/>
<div id="objects" style="width:100%; height:400px;"></div>

<script type="text/javascript">
$(function () {
  $('#usage').highcharts('Chart', {
    title: { text: 'Memory Usage' },
    xAxis: {
      title: {
        text: 'time'
      },
      type: 'datetime'
    },

    series: [{
      name: 'mb',
      data: <%= @usage_data %>,
    }]
  });

  $('#objects').highcharts('Chart', {
    title: { text: 'Object allocations' },
    xAxis: {
      title: {
        text: 'time'
      },
      type: 'datetime'
    },
    series: [{
      name: 'retained',
      data: <%= @object_data %>,
    },
    ]
  });

});
</script>
